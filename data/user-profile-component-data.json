  "dependencies": [],
  "templates": [
    {
      "id": "user-profile-component-template",
      "templateName": "Home Screen",
      "componentName": "UserProfileComponent",
      "imports": [
        "import { useNavigation } from '@react-navigation/native';",
        "import Route from '@/navigation/routes';",
        "import { fonts } from '@/assets/fonts';",
        "import { UserProfileComponent } from 'react-native-user-profile-component';",
        "import { colors } from '@/assets/Colors';",
        "import { Button } from 'react-native-theme-component';"
      ],
      "headers": [
        "  const handleButtonPress = (showFinance: boolean) => {",
        "    // Navigate to the Dashboard screen with the button name as a parameter",
        "    // navigation.navigate('Dashboard', { isFinance: showFinance });",
        "    navigation.navigate(Route.BOTTOM_NAVIGATOR, {",
        "        screen: Route.ACCOUNT_SUMMARY_SCREEN,",
        "        params: { isFinance: showFinance },",
        "      });",
        "  };"
      ],
      "renders": [
        "        <>",
        "            <View style={{flex: 1,justifyContent: 'center',alignItems: 'center',backgroundColor: colors.White}}>",
        "               <UserProfileComponent /> ",
        "               <View style={{",
        "                          flex: 1,",
        "                          flexDirection:'row',",
        "                          alignItems: 'center',",
        "                        }}>",
        "                      <Button",
        "                        testID={'btn-deposit'}",
        "                        label='Deposit'",
        "                        onPress={$isAsync() => $onDepositClick}",
        "                        variant= 'primary'",
        "                        style={{",
        "                          primaryContainerStyle: {",
        "                            flex: 1,",
        "                            marginHorizontal:20",
        "                          }",
        "                        }}",
        "                      />",
        "                      <Button",
        "                        testID={'btn-finance'}",
        "                        label='Finance'",
        "                        onPress={$isAsync() => $onFinanceClick}",
        "                        variant= 'primary'",
        "                        style={{",
        "                          primaryContainerStyle: {",
        "                            flex: 1,",
        "                            marginHorizontal:20",
        "                          }",
        "                        }}",
        "                      />",
        "                      </View>",
        "            </View>",
        "        </>"
      ],
      "functions": [
        {
          "id": "onDepositClick"
        },
        {
          "id": "onFinanceClick"
        }
      ]
    },
    {
      "id": "user-profile-form-component",
      "templateName": "Home Screen",
      "componentName": "UserProfileComponent",
      "imports": [
        "import { useNavigation } from '@react-navigation/native';",
        "import Route from '@/navigation/routes';",
        "import { fonts } from '@/assets/fonts';",
        "$component-imports",
        "import { colors } from '@/assets/Colors';",
        "import { Button,Layouts } from 'react-native-theme-component';"
      ],
      "headers": [
        "  const handleButtonPress = (showFinance: boolean) => {",
        "    // Navigate to the Dashboard screen with the button name as a parameter",
        "    // navigation.navigate('Dashboard', { isFinance: showFinance });",
        "    navigation.navigate(Route.USER_PROFILE_SCREEN);",
        "  };"
      ],
      "renders": [
        "        <>",
        "        <Layouts type='form'>",
        "            $component-name",
        "            <Layouts type='bottom-sticky-content'>",
        "                      <Button",
        "                        testID={'btn-finance'}",
        "                        label='Profile Summary'",
        "                        onPress={() => handleButtonPress(true)}",
        "                        variant= 'primary'",
        "                        style={{",
        "                          primaryContainerStyle: {",
        "                            flex: 1,",
        "                            marginHorizontal:20",
        "                          }",
        "                        }}",
        "                      />",
        "               </Layouts>",
        "         </Layouts>",
        "        </>"
      ],
      "functions": []
    },
