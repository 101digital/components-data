        "                        variant= 'primary'",
        "                        style={{",
        "                          primaryContainerStyle: {",
        "                            flex: 1,",
        "                            marginHorizontal:20",
        "                          }",
        "                        }}",
        "                      />",
        "                      </Layouts>",
        "        </>"
      ],
      "functions": []
