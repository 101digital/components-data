    "env": {
      "development": {
        "membershipUrl": "https://api-neobank-dev.101digital.io/membership-service/1.0.13"
      },
      "staging": {
        "membershipUrl": "https://api-neobank-dev.101digital.io/membership-service/1.0.13"
